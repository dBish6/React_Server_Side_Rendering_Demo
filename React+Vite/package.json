  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/dBish6/React_Server_Side_Rendering_Demo"
  },
  "workspaces": [
    "app/front-end",
    "app/back-end"
  ],
  "scripts": {
    "dev": "npm --workspace back-end run dev",
    "serve": "npm --workspace back-end run serve",
    "build": "npm run build --workspace front-end --workspace back-end",
    "build:front": "npm --workspace front-end run build",
    "build:back": "npm --workspace back-end run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rmdir /s /q node_modules"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vite-aliases": "^0.11.3"
  }
}
