{
  "name": "react_server_side_rendering_demos",
  "private": true,
  "version": "1.0.0",
  "author": "David Bishop",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/dBish6/React_Server_Side_Rendering_Demo"
  },
  "workspaces": [
    "app/front-end",
    "app/back-end"
  ],
  "scripts": {
    "dev:front": "npm --workspace front-end run dev",
    "dev:back": "npm --workspace back-end run dev",
    "serve": "npm run serve --workspace front-end --workspace back-end",
    "serve:front": "npm --workspace front-end run serve",
    "serve:back": "npm --workspace back-end run serve",
    "build": "npm run build --workspace front-end --workspace back-end",
    "build:front": "npm --workspace front-end run build",
    "build:back": "npm --workspace back-end run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rmdir /s /q node_modules"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.19",
    "@types/node": "^20.8.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "esm": "^3.2.25",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
