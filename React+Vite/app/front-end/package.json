{
  "name": "front-end",
  "private": true,
  "version": "1.0.0",
  "main": "ssr.ts",
  "type": "module",
  "proxy": "http://localhost:4000",
  "scripts": {
    "dev": "NODE_ENV=development node --trace-warnings --loader ts-node/esm --experimental-specifier-resolution=node ./ssr.ts",
    "build": "tsc && vite build && tsc && vite build --outDir build/ssr --ssr ../front-end/src/entry-server",
    "serve": "NODE_ENV=production node --trace-warnings --loader ts-node/esm --experimental-specifier-resolution=node ./ssr.ts"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "sirv": "^2.0.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "vite": "^4.4.5"
  }
}
